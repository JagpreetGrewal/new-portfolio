{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^3.8.1",
    "@emotion/react": "^11.14.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.7.5",
    "classnames": "^2.5.1",
    "dotenv": "^16.4.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter": "^0.0.1-security.0",
    "history": "^5.3.0",
    "jest": "^27.5.1",
    "polyfill": "^0.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^18.3.1",
    "react-typist-component": "^1.1.1",
    "resend": "^4.0.0",
    "web-streams-polyfill": "^4.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --passWithNoTests --silent=true --watchAll --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "lines": 90
      }
    },
    "moduleNameMapper": {
      "^axios$": "axios/dist/node/axios.cjs"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js"
    ]
  },
  "devDependencies": {
    "cheerio": "^1.0.0-rc.3"
  }
}
